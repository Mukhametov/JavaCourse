
public class TestGeneric {

	abstract class Shape{
		public abstract void draw(); 
	}
	
	class Polygon extends Shape{
		@Override
		public void draw() {
			System.out.println(getClass().getSimpleName());
		}
	}
	
	class Ellipse extends Shape{
		@Override
		public void draw() {
			System.out.println(getClass().getSimpleName());
		}
	}

	class Circle extends Shape{
		@Override
		public void draw() {
			System.out.println(getClass().getSimpleName());
		}
	}

}
