import java.util.Scanner;

public class Run {
	public static void main(String[] args) {
		System.out.print("Введите значение элемента: ");
		Scanner sc = new Scanner(System.in);
		while (true) {
			String com = sc.nextLine();
			String C = com.substring(0, com.indexOf(" "));
			String arguments = com.substring(com.indexOf(" "));

			Operation op = null;
			if (C.equals("Push"))
				op = new Push();
			else if (C.equals("Print"))
				op = new Print();
			else {
				System.err.println("Unknown command!");
				continue;
			}
			op.execute(arguments); // arguments подчеркивает волнистой красной
									// линией
		}

	}

}