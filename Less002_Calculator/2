DEFINE a -1
DEFINE b 0
DEFINE c 16

PUSH b
PUSH b
MUL
SHARP b^2
PRINT

PUSH 4
PUSH a
PUSH c
MUL
MUL
SHARP 4ac
PRINT

SUB
SHARP b^2 - 4ac
PRINT

SQRT
SHARP SQRT(b^2 - 4ac)
PRINT

PUSH -1
MUL
SHARP - SQRT(b^2 - 4ac)
PRINT

PUSH b
SUB
SHARP - SQRT(b^2 - 4ac) - b
PRINT


PUSH 2
PUSH a
MUL
SHARP 2a
PRINT

DIV
SHARP (- SQRT(b^2 - 4ac) - b) / 2a
PRINT
